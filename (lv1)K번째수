def solution(array, commands):
    answer=[]
    for i in range(len(commands)):
        
        cnt=[]
        for j in range(commands[i][0]-1,commands[i][1]):
            cnt.append(array[j])
        
        cnt.sort()
        answer.append(cnt[commands[i][2]-1])  
        cnt.clear()
    
        
    
    return answer
